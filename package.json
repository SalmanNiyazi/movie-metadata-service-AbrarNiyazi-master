{
	"name": "movie-metadata-service-abrarniyazi-master",
	"version": "1.0.0",
	"description": "Get merged data from two sources on a single endpoint",
	"main": "index.ts",
	"scripts": {
		"build": "tsc",
		"start": "tsc && node dist/src/index.js",
		"dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/index.js\"",
		"lint": "eslint --ignore-path .eslintignore --ext .js,.ts .",
		"format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\""
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/SalmanNiyazi/movieMetadata.git"
	},
	"author": "Salman Niyazi",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/SalmanNiyazi/movieMetadata/issues"
	},
	"homepage": "https://github.com/SalmanNiyazi/movieMetadata#readme",
	"dependencies": {
		"axios": "^1.0.0",
		"dotenv": "^16.0.2",
		"express": "^4.18.1"
	},
	"devDependencies": {
		"@types/axios": "^0.14.0",
		"@types/express": "^4.17.14",
		"@types/node": "^18.7.23",
		"@typescript-eslint/eslint-plugin": "^5.39.0",
		"@typescript-eslint/parser": "^5.39.0",
		"concurrently": "^7.4.0",
		"eslint": "^8.24.0",
		"eslint-config-prettier": "^8.5.0",
		"nodemon": "^2.0.20",
		"prettier": "^2.7.1",
		"typescript": "^4.8.4"
	}
}
